{
  "name": "training",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development&& npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "SET NODE_ENV=production&& npm-run-all clean build server:prod",
    "watch": "nodemon",
    "lint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "dependencies": {
    "body-parser": "latest",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "latest",
    "mongoose": "^5.6.13",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "rest-api-errors": "^1.2.1",
    "rimraf": "^3.0.0",
    "socket.io": "^2.2.0",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.2",
    "prettier": "1.18.2"
  }
}
